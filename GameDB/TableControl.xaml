                                </StackPanel>

                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontFamily="/GameDB;component/Resources/#Marvin" FontSize="15" Margin="10, 5" Text="Name:" Foreground="#FF564545" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100"/>
                                    <TextBox x:Name="user_name" FontFamily="/GameDB;component/Resources/#Marvin" FontSize="15" Margin="10, 5" Text="" Foreground="#FF564545" HorizontalAlignment="Left" VerticalAlignment="Center" Width="240" MaxLength="30"/>
                                </StackPanel>

                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontFamily="/GameDB;component/Resources/#Marvin" FontSize="15" Margin="10, 5" Text="Surname:" Foreground="#FF564545" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100"/>
                                    <TextBox x:Name="user_surname" FontFamily="/GameDB;component/Resources/#Marvin" FontSize="15" Margin="10, 5" Text="" Foreground="#FF564545" HorizontalAlignment="Left" VerticalAlignment="Center" Width="240" MaxLength="30"/>
                                </StackPanel>

                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontFamily="/GameDB;component/Resources/#Marvin" FontSize="15" Margin="10, 5" Text="Donat:" Foreground="#FF564545" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100"/>
                                    <TextBox x:Name="user_donat" FontFamily="/GameDB;component/Resources/#Marvin" FontSize="15" Margin="10, 5" Text="" Foreground="#FF564545" HorizontalAlignment="Left" VerticalAlignment="Center" Width="240" MaxLength="30"/>
                                </StackPanel>

                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontFamily="/GameDB;component/Resources/#Marvin" FontSize="15" Margin="10, 5" Text="Role:" Foreground="#FF564545" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100"/>
                                    <TextBox x:Name="user_role" FontFamily="/GameDB;component/Resources/#Marvin" FontSize="15" Margin="10, 5" Text="" Foreground="#FF564545" HorizontalAlignment="Left" VerticalAlignment="Center" Width="240" MaxLength="30"/>
                                </StackPanel>
                            </WrapPanel>
                        </GroupBox>

                        <DataGrid x:Name="userTable" Margin="10, 10, 10, 0" Grid.Column="1" Grid.Row="0" SelectionChanged="UserTable_SelectionChanged" IsReadOnly="True"/>

                        <StackPanel x:Name="userPanel" Orientation="Horizontal" Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2">
                            <Button Name="userInsertButton" Content="Insert" FontFamily="/GameDB;component/Resources/#Marvin" FontSize="20" Margin="10, 10" BorderBrush="#FF268A9D" Background="#FF175E6B" Foreground="White" Click="UserInsertClick" IsEnabled="True" Width="340" Height="40"/>
                            <Button Name="userUpdateButton" Content="Update" FontFamily="/GameDB;component/Resources/#Marvin" FontSize="20" Margin="10, 10" BorderBrush="#FF268A9D" Background="#FF175E6B" Foreground="White" Click="UserUpdateClick" IsEnabled="False" Width="340" Height="40"/>
                            <Button Name="userDeleteButton" Content="Delete" FontFamily="/GameDB;component/Resources/#Marvin" FontSize="20" Margin="10, 10" BorderBrush="#FF268A9D" Background="#FF175E6B" Foreground="White" Click="UserDeleteClick" IsEnabled="False" Width="340" Height="40"/>
                        </StackPanel>
                    </Grid>
                </TabItem>

